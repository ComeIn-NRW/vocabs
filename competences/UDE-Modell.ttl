@base <https://w3id.org/comein/vocabs/ude-modell/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<> a skos:ConceptScheme ;
    dct:title "UDE-Modell: Ein integratives Modell digitalisierungsbezogener Kompetenzen für die Lehramtsausbildung"@de ;
    dct:created "2021-09-17"^^xsd:date;
    dct:modified "2021-09-17"^^xsd:date;
    dct:issued "2021-09-17"^^xsd:date;
    dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept <a>, <b1>, <b2>, <c> .

<a> a skos:Concept ;
    skos:prefLabel "Fachspezifische, fächerübergreifende und überfachliche Basiskompetenzen zu digitalen Technologien (Analyse – Reflexion – Gestaltung)"@de ;
    skos:topConceptOf <> ;
    skos:narrower <a1>, <a2>, <a3> .

<a1> a skos:Concept ;
    skos:prefLabel "Technologische und mediale Strukturen und Funktionen"@de ;
    skos:broader <a> ;
    skos:inScheme <> .

<a2> a skos:Concept ;
    skos:prefLabel "Gesellschaftliche und kulturelle Wechselwirkungen"@de ;
    skos:broader <a> ;
    skos:inScheme <> .

<a3> a skos:Concept ;
    skos:prefLabel "Interaktion: Nutzung, Handlung, Subjektivierung"@de ;
    skos:broader <a> ;
    skos:inScheme <> .

<b1> a skos:Concept ;
    skos:prefLabel "Lehren und Lernen"@de ;
    skos:topConceptOf <> ;
    skos:narrower <b1.1>, <b1.2>, <b1.3> .

<b2> a skos:Concept ;
    skos:prefLabel "Berufliches Engagement"@de ;
    skos:topConceptOf <> ;
    skos:narrower <b2.4>, <b2.5>, <b2.6> .

<c> a skos:Concept ;
    skos:prefLabel "Kritisch-konstruktive Praxis und deren Reflexion"@de ;
    skos:topConceptOf <> ;
    skos:narrower <c.7> .

<b1.1> a skos:Concept ;
    skos:prefLabel "Unterrichtsplanung"@de ;
    skos:definition "Digitale Technologien gezielt zur Planung von Unterricht nutzen."@de ;
    skos:broader <b1> ;
    skos:narrower <b1.11>, <b1.12>, <b1.13>, <b1.14>, <b1.15> ;
    skos:inScheme <> .

<b1.11> a skos:Concept ;
    skos:prefLabel "Potenzial erkennen"@de ;
    skos:definition "Das (didaktische) Potenzial digitaler Technologien als Erweiterung der traditionellen Lehr- und Lernmedien erkennen."@de ;
    skos:broader <b1.1> ;
    skos:inScheme <> .

<b1.12> a skos:Concept ;
    skos:prefLabel "Identifizieren"@de ;
    skos:definition "Geeignete digitale Lehr- und Lerntechnologien und -ressourcen identifizieren und in die Formulierung des Ausstattungsbedarfs der Schule einbringen."@de ;
    skos:broader <b1.1> ;
    skos:inScheme <> .

<b1.13> a skos:Concept ;
    skos:prefLabel "Lernbedürfnisse und Lerngruppen berücksichtigen"@de ;
    skos:definition "Den Einsatz digitaler Technologien im Lehr-Lern-Prozess planen, um individuelles Lernen zu fördern und die Heterogenität und die vielfältigen Lernbedürfnisse von Lernenden, die Lerngruppe sowie die (digitalisierungsbezogenen) Erwartungen, Fähigkeiten, Anwendungserfahrungen und (Fehl-)Vorstellungen von Lernenden berücksichtigen"@de ;
    skos:broader <b1.1> ;
    skos:inScheme <> .

<b1.14> a skos:Concept ;
    skos:prefLabel "Digitale Ressourcen modifizieren"@de ;
    skos:definition "Vorhandene digitale Ressourcen (z. B. offene Bildungsressourcen (OER)) unter Verwendung digitaler Werkzeuge modifizieren und weiterentwickeln bzw. mitgestalten."@de ;
    skos:broader <b1.1> ;
    skos:inScheme <> .

<b1.15> a skos:Concept ;
    skos:prefLabel "Digitale Ressourcen entwickeln"@de ;
    skos:definition "Neue digitale Bildungsressourcen, Lehr-Lern-Formate und Unterrichtsmethoden entwickeln und erstellen."@de ;
    skos:broader <b1.1> ;
    skos:inScheme <> .

<b1.2> a skos:Concept ;
    skos:prefLabel "Unterrichtsgestaltung"@de ;
    skos:definition "Digitalisierung als Unterrichtsthema verankern und digitale Technologien zur Bereicherung von Lehren und Lernen nutzen."@de ;
    skos:broader <b1> ;
    skos:narrower <b1.21>, <b1.22>, <b1.23>, <b1.24>, <b1.25>, <b1.26>, <b1.27>, <b1.28>, <b1.29> ;
    skos:inScheme <> .

<b1.21> a skos:Concept ;
    skos:prefLabel "Digitalisierung als Lerngegenstand aus fachlicher Perspektive konkretisieren und im Unterricht integrieren"@de ;
    skos:definition "Die Inhalte der eigenen Unterrichtsfächer für den Lerngegenstand Digitales definieren und gewinnbringend einsetzen. An Digitalisierung als Lerngegenstand an geeigneter Stelle im Unterricht anknüpfen."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.22> a skos:Concept ;
    skos:prefLabel "Digitale Technologien zur Veranschaulichung nutzen"@de ;
    skos:definition "Digitale Technologien zur Veranschaulichung von Unterrichtsinhalten insbesondere dort nutzen, wo diese durch ihre spezifische Darstellungsform zum Verständnis des Sachverhalts beitragen können."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.23> a skos:Concept ;
    skos:prefLabel "Kooperatives und kollaboratives Lernen fördern"@de ;
    skos:definition "Digitale Technologien nutzen, um die Zusammenarbeit von Lehrenden und Lernenden (auch untereinander) sowie kollaborative Lernstrategien zu fördern und zu verbessern. Lernende befähigen, digitale Technologien im Rahmen von Gruppen- und Projektarbeiten zu nutzen, um die Kommunikation, Kooperation und den gemeinsamen Wissenserwerb innerhalb der Lerngruppe zu organisieren."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.24> a skos:Concept ;
    skos:prefLabel "Selbstgesteuertes Lernen fördern"@de ;
    skos:definition "Digitale Technologien nutzen, um selbstgesteuerte Lernprozesse zu unterstützen, d. h. den Lernenden zu ermöglichen, ihr eigenes Lernen zu planen, zu überprüfen und zu reflektieren, Fortschritte zu dokumentieren, Erkenntnisse zu kommunizieren und kreative Lösungen zu erarbeiten."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.25> a skos:Concept ;
    skos:prefLabel "Inklusion fördern"@de ;
    skos:definition "Hürden bzw. Einschränkungen bei der Verwendung digitaler Technologien und der Vermittlung digitalisierungsbezogener Unterrichtsinhalte identifizieren, um diese zu verringern bzw. zu berücksichtigen und durch digitale Medien z. B. Lernbeeinträchtigungen adäquat zu begegnen."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.26> a skos:Concept ;
    skos:prefLabel "Zugänglichkeit sicherstellen"@de ;
    skos:definition "Beachten, dass alle Lernenden zu (den eingesetzten) digitalen Lernressourcen und -aktivitäten einen Zugang benötigen, bzw. Alternativen aufzeigen."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.27> a skos:Concept ;
    skos:prefLabel "Feedback und Beratung anbieten und weiterentwickeln"@de ;
    skos:definition "Digitale Technologien nutzen, um den Lernenden Beratung anzubieten sowie rechtzeitig/zeitnah und gezielt Feedback zu geben. Neue digitale Formen und Formate der Hilfestellung und Anleitung kreativ erproben und nutzen sowie schon vorhandene Formen und Formate nachhaltig weiterentwickeln."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.28> a skos:Concept ;
    skos:prefLabel "Organisieren und Didaktisieren"@de ;
    skos:definition "Digitalisierungsbezogene didaktische Maßnahmen (in ihrem Zusammenspiel) organisieren."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.29> a skos:Concept ;
    skos:prefLabel "Kreativ, problemlöseorientiert und ergebnisoffen mit digitalen Lehr-Lern-Formaten umgehen"@de ;
    skos:definition "(Neue) Digitale Ressourcen, Lehr-Lern-Formate und digital gestützte Unterrichtsmethoden ergebnisoffen und kreativ in den Unterricht integrieren sowie diese weiterentwickeln hin zu einer produktiven Sicherstellung von methodischer Vielfalt."@de ;
    skos:broader <b1.2> ;
    skos:inScheme <> .

<b1.3> a skos:Concept ;
    skos:prefLabel "Assessment, Evaluation & Revision"@de ;
    skos:definition "Digitale Technologien für die Beurteilung und Bewertung von Schüler*innenleistungen und die Evaluation des eigenen Unterrichts nutzen und auf dieser Basis digital gestützt den eigenen Unterricht weiterentwickeln und modifizieren."@de ;
    skos:broader <b1> ;
    skos:narrower <b1.31>, <b1.32>, <b1.33>, <b1.34> ;
    skos:inScheme <> .

<b1.31> a skos:Concept ;
    skos:prefLabel "Schüler*innenleistung(en) beurteilen und diagnostizieren"@de ;
    skos:definition "Digitale Technologien für die formative und summative Leistungsbeurteilung und Diagnose von den zu vermittelnden Kompetenzen verwenden."@de ;
    skos:broader <b1.3> ;
    skos:inScheme <> .

<b1.32> a skos:Concept ;
    skos:prefLabel "Evidenzbasiert evaluieren"@de ;
    skos:definition "Mit Hilfe digitaler Technologien Evidenzen zu Lerner*innenaktivitäten, Leistung und Fortschritt generieren, analysieren, kritisch interpretieren und für das eigene Vorangehen nutzen."@de ;
    skos:broader <b1.3> ;
    skos:inScheme <> .

<b1.33> a skos:Concept ;
    skos:prefLabel "Unterrichtsplanung und -strategien anpassen"@de ;
    skos:definition "Auf Basis der durch digitale Technologien erzeugten Evidenzen die eigenen Unterrichtsstrategien anpassen und Lernende gezielt unterstützen."@de ;
    skos:broader <b1.3> ;
    skos:inScheme <> .

<b1.34> a skos:Concept ;
    skos:prefLabel "Beurteilungsformate analysieren und adaptieren"@de ;
    skos:definition "Die Vielfalt und die Angemessenheit digitaler Beurteilungsformate und -ansätze berücksichtigen und ggf. Anpassungen hinsichtlich der Nutzung vornehmen."@de ;
    skos:broader <b1.3> ;
    skos:inScheme <> .

<b2.4> a skos:Concept ;
    skos:prefLabel "Institutionelle Entwicklung"@de ;
    skos:definition "Digitale Technologien gezielt für Organisation und Verwaltung der Bildungsinstitution nutzen."@de ;
    skos:broader <b2> ;
    skos:narrower <b2.41>, <b2.42>, <b2.43>, <b2.44>, <b2.45> ;
    skos:inScheme <> .

<b2.41> a skos:Concept ;
    skos:prefLabel "Analyseinstrumente identifizieren"@de ;
    skos:definition "Geeignete digitale Technologien für die systemische Analyse und Entwicklung der eigenen Schule identifizieren und bewerten."@de ;
    skos:broader <b2.4> ;
    skos:inScheme <> .

<b2.42> a skos:Concept ;
    skos:prefLabel "Analyseinstrumente nutzen"@de ;
    skos:definition "Gezielt digitale Instrumente für die Analyse systemischer Bedingungen nutzen und die Ergebnisse für die (digitalisierungsbezogene) Schulentwicklung verwenden."@de ;
    skos:broader <b2.4> ;
    skos:inScheme <> .

<b2.43> a skos:Concept ;
    skos:prefLabel "Digitalisierungsstrategie entwickeln und umsetzen"@de ;
    skos:definition "Den Entwurf und die Umsetzung der Digitalisierungsstrategie der Schule (mit-)gestalten und dabei die Perspektive des Faches einbringen."@de ;
    skos:broader <b2.4> ;
    skos:inScheme <> .

<b2.44> a skos:Concept ;
    skos:prefLabel "Digitalisierungsstrategie darstellen"@de ;
    skos:definition "Die schulische Digitalisierungsstrategie adressat*innengerecht und begründet darstellen."@de ;
    skos:broader <b2.4> ;
    skos:inScheme <> .

<b2.45> a skos:Concept ;
    skos:prefLabel "Öffentlichkeitsarbeit gestalten"@de ;
    skos:definition "Digitale Technologien inhaltlich adäquat und adressat*innengerecht nutzen, um Öffentlichkeitsarbeit zu gestalten."@de ;
    skos:broader <b2.4> ;
    skos:inScheme <> .

<b2.5> a skos:Concept ;
    skos:prefLabel "Zusammenarbeit"@de ;
    skos:definition "Digitale Technologien nutzen, um mit anderen Lehrenden, (externen) Kooperationspartner*innen und Beratungsstellen zusammenzuarbeiten."@de ;
    skos:broader <b2> ;
    skos:narrower <b2.51>, <b2.52>, <b2.53>, <b2.54> ;
    skos:inScheme <> .

<b2.51> a skos:Concept ;
    skos:prefLabel "Material austauschen und Inhalte verfügbar machen"@de ;
    skos:definition "Digitale Technologien nutzen, um Wissen, Erfahrungen und Materialien zu teilen und auszutauschen bzw. digitale Inhalte Lernenden, Eltern und anderen Lehrenden zur Verfügung zu stellen."@de ;
    skos:broader <b2.5> ;
    skos:inScheme <> .

<b2.52> a skos:Concept ;
    skos:prefLabel "Didaktische und pädagogische Praktiken erneuern"@de ;
    skos:definition "Digitale Technologien nutzen, um gemeinschaftlich didaktische und pädagogische Praktiken weiterzuentwickeln."@de ;
    skos:broader <b2.5> ;
    skos:inScheme <> .

<b2.53> a skos:Concept ;
    skos:prefLabel "Feedback geben"@de ;
    skos:definition "Digitale Technologien nutzen, um unter Kolleg*innen Peer-Feedback zu geben."@de ;
    skos:broader <b2.5> ;
    skos:inScheme <> .

<b2.54> a skos:Concept ;
    skos:prefLabel "Digitale Kommunikationswege etablieren"@de ;
    skos:definition "Digitale Technologien nutzen, um die (institutionelle/formale) Kommunikation mit Lernenden, Eltern und Dritten (z. B. Kooperationspartner*innen) zu gestalten, zu verbessern und zur Weiterentwicklung und Bereicherung beruflicher Kommunikationsstrategien beizutragen."@de ;
    skos:broader <b2.5> ;
    skos:inScheme <> .

<b2.6> a skos:Concept ;
    skos:prefLabel "Lebenslanges Lernen"@de ;
    skos:definition "Digitale Angebote gezielt für die eigene Fort- und Weiterbildung nutzen."@de ;
    skos:broader <b2> ;
    skos:narrower <b2.61>, <b2.62> ;
    skos:inScheme <> .

<b2.61> a skos:Concept ;
    skos:prefLabel "Digitale Fort- und Weiterbildungsangebote identifizieren und nutzen"@de ;
    skos:definition "Die eigenen digitalisierungsbezogenen Kompetenzen reflektieren und geeignete Fortbildungsangebote sowie digitale Technologien und Ressourcen für die eigene Weiterbildung identifizieren und nutzen."@de ;
    skos:broader <b2.6> ;
    skos:inScheme <> .

<b2.62> a skos:Concept ;
    skos:prefLabel "Lernstrategien nutzen"@de ;
    skos:definition "Kenntnis von Lernstrategien haben bzw. Lernstrategien nutzen, mit denen der (unterrichtsbezogene) Gebrauch und Einsatz neuer digitaler Technologien (möglichst selbstständig) erlernt werden kann."@de ;
    skos:broader <b2.6> ;
    skos:inScheme <> .

<c.7> a skos:Concept ;
    skos:prefLabel "Reflexion digitalisierungsbezogener Maßnahmen"@de ;
    skos:definition "Vor- und Nachteile digitalisierungsbezogener Maßnahmen unter Berücksichtigung der kontextuellen Rahmenbedingungen umfassend reflektieren und in alle Entscheidungsprozesse einbeziehen."@de ;
    skos:broader <c> ;
    skos:narrower <c.71>, <c.72>, <c.73>, <c.74>, <c.75> ;
    skos:inScheme <> .

<c.71> a skos:Concept ;
    skos:prefLabel "Eine kritische und zugleich offene Haltung praktizieren"@de ;
    skos:definition "Dem Einsatz digitaler Technologien im Berufsfeld offen und zugleich (selbst-)kritisch und reflektiert begegnen."@de ;
    skos:broader <c.7> ;
    skos:inScheme <> .

<c.72> a skos:Concept ;
    skos:prefLabel "Die eigene berufliche Praxis auch mittels digitaler Technologien gestalten"@de ;
    skos:definition "Selbstreguliert das Potenzial digitaler Technologien der Situation angemessen für das Lehren und Lernen, die eigene Professionalisierung, die Arbeitsorganisation, die Zusammenarbeit und die Weiterentwicklung der Schule ausschöpfen."@de ;
    skos:broader <c.7> ;
    skos:inScheme <> .

<c.73> a skos:Concept ;
    skos:prefLabel "Motivieren und unterstützen"@de ;
    skos:definition "Unter Einbezug individueller Motivation, Kenntnisse und kontextueller Gegebenheiten Kolleg*innen dazu motivieren und dabei unterstützen, (neue) digitale Technologien zu testen, zu entwickeln und zu nutzen sowie diese (gemeinsam) kritisch zu reflektieren."@de ;
    skos:broader <c.7> ;
    skos:inScheme <> .

<c.74> a skos:Concept ;
    skos:prefLabel "Reflexion über digitale Evaluationsinstrumente"@de ;
    skos:definition "Die Aussagekraft konkreter digitaler Instrumente zur Evaluation von Lernleistungen, Lehr-Lern-Umgebungen und systemischen Bedingungen prüfen und diese gegebenenfalls um weitere Verfahren erweitern bzw. modifizieren."@de ;
    skos:broader <c.7> ;
    skos:inScheme <> .

<c.75> a skos:Concept ;
    skos:prefLabel "Ethische und rechtliche Rahmenbedingungen berücksichtigen"@de ;
    skos:definition "Die eigene Verwendung und den Einsatz digitaler Technologien (im Unterricht) hinsichtlich ethischer und rechtlicher Fragestellungen reflektieren."@de ;
    skos:broader <c.7> ;
    skos:inScheme <> .
