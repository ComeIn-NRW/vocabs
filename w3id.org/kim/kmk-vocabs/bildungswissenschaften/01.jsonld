{
  "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/01",
  "type": "Concept",
  "followers": "https://test.skohub.io/followers?subject=vocabs%2Fw3id.org%2Fkim%2Fkmk-vocabs%2Fbildungswissenschaften%2F01",
