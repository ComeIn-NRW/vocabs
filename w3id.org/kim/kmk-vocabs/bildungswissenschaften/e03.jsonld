{
  "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e03",
  "type": "Concept",
  "followers": "https://test.skohub.io/followers?subject=vocabs%2Fw3id.org%2Fkim%2Fkmk-vocabs%2Fbildungswissenschaften%2Fe03",
  "inbox": "https://test.skohub.io/inbox?target=vocabs%2Fw3id.org%2Fkim%2Fkmk-vocabs%2Fbildungswissenschaften%2Fe03",
  "prefLabel": {
    "de": "Lehrkräfte finden alters- und entwicklungspsychologisch adäquate Lösungsansätze für Schwierigkeiten und Konflikte in Schule und Unterricht und tragen zu einem wertschätzenden Umgang bei."
  },
  "narrower": [
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0302",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen kennen Regeln der Gesprächsführung sowie Grundsätze des Umgangs miteinander, die in Unterricht, Schule und Elternarbeit bedeutsam sind."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0303",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen kennen Risiken sowie Potenziale im Kindes- und Jugendalter sowie Präventions-, Interventions- und Unterstützungsmöglichkeiten."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0304",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen analysieren Konflikte und kennen Methoden der konstruktiven Konfliktbearbeitung sowie des Umgangs mit Gewalt und Diskriminierung."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0305",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen kennen Chancen und Risiken der Mediennutzung in Bezug auf Emotionen, Verhalten und Einstellungen sowie Konzepte des konstruktiven Umgangs damit."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0306",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen gestalten soziale Beziehungen und soziale Lernprozesse in Unterricht und Schule."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0307",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen erarbeiten mit den Schülerinnen und Schülern Regeln des wertschätzenden Umgangs miteinander und setzen diese um."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0308",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen wenden im konkreten Fall Strategien und Handlungsformen der Konfliktprävention und -lösung an."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0309",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen gestalten und vermitteln Lösungsansätze zum Umgang mit Chancen und Risiken der Mediennutzung."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/e0301",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen verfügen über Kenntnisse zu Kommunikation und Interaktion (unter besonderer Berücksichtigung der Lehrer-Schüler-Interaktion)."
      }
    }
  ],
  "broader": {
    "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/02",
    "prefLabel": {
      "de": "Erziehen"
    }
  },
