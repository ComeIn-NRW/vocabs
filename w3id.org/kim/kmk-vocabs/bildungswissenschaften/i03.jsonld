  "prefLabel": {
    "de": "Lehrkräfte beteiligen sich an der Schul- und Unterrichtsentwicklung."
  },
  "narrower": [
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/i0301",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen kennen und reflektieren den spezifischen Bildungsauftrag verschiedener Schularten und Bildungsgänge."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/i0302",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen kennen Ziele, Methoden, Rahmenbedingungen und Prozessabläufe der Schulund Unterrichtsentwicklung und reflektieren die Herausforderungen auch unter dem Aspekt inklusiver Schulentwicklung."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/i0303",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen wenden Ergebnisse der Schul-, Unterrichts- und Bildungsforschung auf die Gestaltung von Schul- und Unterrichtsentwicklung an."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/i0304",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen nutzen Verfahren und Instrumente der internen Evaluation zur Qualitätsentwicklung von Unterricht und Schule."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/i0305",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen planen schulische Projekte und Vorhaben kooperativ und setzen sie um."
      }
    },
    {
      "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/i0306",
      "prefLabel": {
        "de": "Die Absolventinnen und Absolventen können schulische Innovationsprozesse mitgestalten und erproben reflektiert neue Konzepte, Anwendungen und Technologien."
      }
    }
  ],
  "broader": {
    "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/04",
    "prefLabel": {
      "de": "Innovieren"
    }
  },
  "inScheme": {
    "id": "http://w3id.org/kim/kmk-vocabs/bildungswissenschaften/",
    "title": {
      "de": "KMK-Standards für die Lehrerbildung: Bildungswissenschaften (vom 16.12.2004 i.d.F. vom 16.05.2019)"
    }
  },
